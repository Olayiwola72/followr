<p id="notice"><%= notice %></p>

<h1>Listing Twitter Follows</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twitter_follows.each do |twitter_follow| %>
      <tr>
        <td><%= link_to 'Show', twitter_follow %></td>
        <td><%= link_to 'Edit', edit_twitter_follow_path(twitter_follow) %></td>
        <td><%= link_to 'Destroy', twitter_follow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twitter follow', new_twitter_follow_path %>
