<p id="notice"><%= notice %></p>

<h1>Listing Twitter Follow Preferences</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twitter_follow_preferences.each do |twitter_follow_preference| %>
      <tr>
        <td><%= link_to 'Show', twitter_follow_preference %></td>
        <td><%= link_to 'Edit', edit_twitter_follow_preference_path(twitter_follow_preference) %></td>
        <td><%= link_to 'Destroy', twitter_follow_preference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twitter follow preference', new_twitter_follow_preference_path %>
